@model List<Student>

<a asp-action="AddStudent" class=" btn btn-info">Add Student</a>


<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Course</th>
            <th scope="col">Fees</th>
            <th scope="col">Action</th>

        </tr>
    </thead>
    <tbody>
      @foreach (var d in Model)
      {
          <tr>
                <th scope="row">@d.Sid</th>
                <td>@d.SName</td>
                <td>@d.Scourse</td>
                <td>@d.Sfees</td>
                <td><a class="btn btn-sm btn-danger" asp-action="DelStudent" asp-route-id="@d.Sid"> Delete</a></td>
                <td><a class="btn btn-sm btn-danger" asp-action="EditStudent" asp-route-id="@d.Sid"> Edit</a></td>
          </tr>
      }
    </tbody>
</table>